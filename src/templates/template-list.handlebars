<table class="table table-striped table-templates">
  <thead>
    <tr>
      <th>Name</th>
      <th>ID</th>
      <th>Description</th>
      <th>Version</th>
    </tr>
  </thead>
  <tbody>
    {{#each templates}}
    <tr>
      <td class="name">
        <img class="icon" alt="Icon" src="{{dashicon}}" />
        {{#if value.dashboard.title}}
          {{value.dashboard.title}}
        {{else}}
          {{id}}
        {{/if}}
      </td>
      <td>{{id}}</td>
      <td>{{value.dashboard.description}}</td>
      <td>{{value.dashboard.version}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
